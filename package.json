    "@nestjs/mongoose": "^11.0.3",
    "@nestjs/passport": "^11.0.5",
    "@nestjs/platform-express": "^11.1.1",
    "@nestjs/serve-static": "^5.0.3",
    "axios": "^1.9.0",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.2",
    "exceljs": "^4.4.0",
    "fs-extra": "^11.3.0",
    "moment-timezone": "^0.5.48",
    "mongoose": "^8.15.0",
    "passport": "^0.7.0",
